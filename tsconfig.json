{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./",
    "sourceMap": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["ESNext", "dom"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "downlevelIteration": true,
    "module": "esNext",
    "target": "es6",
    "skipLibCheck": true,
    "jsx": "preserve"
  },
  "files": [
      "callable.d.ts",
      "repository.d.ts",
      "repka.d.ts",
      "watcher.d.ts",
      "create.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}


